{
  "name": "roverz-container-geolocation",
  "version": "0.0.1",
  "description": "geolocation container",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pretest": "./node_modules/.bin/eslint -c .eslintrc --ext .js src",
    "test": "jest"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elarasu/roverz-container-geolocation.git"
  },
  "keywords": [
    "geolocation",
    "container"
  ],
  "author": "elarasu",
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button,react-native-device-info|react-clone-referenced-element)"
    ],
    "collectCoverage": true,
    "verbose": true
  },
  "peerDependencies": {
    "react-native": ">=0.28.0"
  },
  "dependencies": {
    "react-native-background-geolocation": "2.8.1"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "3.0.1",
    "jest": "^20.0.4",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "^16.0.0-alpha.6"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
